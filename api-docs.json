						"required": true,
						"type": "string"
					},
					{
						"name": "merchantReference ",
						"in": "query",
						"description": "external reference from the merchant",
						"required": true,
						"type": "string"
					},
					{
						"name": "message ",
						"in": "query",
						"description": "An optional message describing the reason for the cancelation.",
						"required": false,
						"type": "string"
					}
				],
				"responses": {
					"200": {
