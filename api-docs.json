						"required": true,
						"type": "string"
					},
					{
						"name": "merchantReference ",
						"in": "query",
						"description": "external reference from the merchant",
						"required": true,
						"type": "string"
					},
					{
						"name": "amount ",
						"in": "query",
						"description": "The amount to be captured",
						"required": false,
